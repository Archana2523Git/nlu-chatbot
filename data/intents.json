      "responses": ["Here are your pending tasks."]
    },
    {
      "tag": "check_time",
      "patterns": [
        "what time is it now",
        "tell me the current time",
        "what is the time in chennai"
      ],
      "responses": ["Here is the current time."]
    },
    {
      "tag": "check_date",
      "patterns": [
        "what is today's date",
        "tell me the date today",
        "which day is it today"
      ],
      "responses": ["Here is today's date."]
    },
    {
      "tag": "greeting",
      "patterns": [
        "hi",
        "hello there",
        "good morning",
        "hey bot"
      ],
      "responses": ["Hello! How can I help you?"]
    },
    {
      "tag": "goodbye",
      "patterns": [
        "bye",
        "see you later",
        "good night",
        "talk to you soon"
      ],
      "responses": ["Goodbye! Have a nice day."]
    },
    {
      "tag": "thanks",
      "patterns": [
        "thank you",
        "thanks a lot",
        "that's helpful thanks",
        "appreciate your help"
      ],
      "responses": ["You're welcome!"]
    },
    {
      "tag": "help",
      "patterns": [
        "what can you do",
        "how can you help me",
        "i need some help",
        "show me your features"
      ],
      "responses": ["I can help you with flights, hotels, cabs, food orders, and more."]
    },
    {
      "tag": "small_talk_how_are_you",
      "patterns": [
        "how are you",
        "how is it going",
        "how are you doing today"
      ],
      "responses": ["I'm good, thanks for asking!"]
    },
    {
      "tag": "small_talk_who_are_you",
      "patterns": [
        "who are you",
        "tell me about yourself",
        "what are you"
      ],
      "responses": ["I'm your friendly NLU chatbot!"]
    },
    {
      "tag": "fallback",
      "patterns": [
        "random text that makes no sense",
        "blah blah blah",
        "i don't know what to say"
      ],
      "responses": ["Sorry, I didn't understand that."]
    }
  ]
}
